{
  "model": {
    "default_value": "bert_tiny",
    "description": "Model to test",
    "valid_values": ["bert_tiny"]
  },
  "dataset": {
    "default_value": "reddit_dep",
    "description": "Path to dataset to use for testing",
    "valid_values": ["reddit_dep"]
  },
  "model_path": {
    "default_value": "client/models/bert_tiny/bert_tiny_best.ckpt",
    "description": "File path to model to test",
    "valid_values": "Any valid path to .ckpt file"
  },
  "test_batch_size": {
    "default_value": 8,
    "description": "Batch size for testing",
    "valid_values": [2, 4, 8, 16, 32, 64]
  },
  "max_length": {
    "default_value": 512,
    "description": "Maximum length of the input sequence",
    "valid_values": "Any valid positive integer"
  },
  "seed": {
    "default_value": 200,
    "description": "Seed for random number generators",
    "valid_values": "Any valid positive integer"
  },
  "device": {
    "default_value": "cuda",
    "description": "Device to use for training",
    "valid_values": ["cpu", "cuda"]
  }
}
